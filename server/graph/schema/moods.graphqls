type Moods {
    id: Int!
    notes: String!
    moodTypeId: Int!
    userId: Int!
}

input MoodsInput {
    notes: String!
}

type MoodType {
    id: Int!
    name: String!
}

input MoodTypeInput {
    name: String!
}

extend type Query {
    getMoods: [Moods!]!
    getMoodById(id: Int!): Moods!
    getMoodTypes: [MoodType!]!
}

extend type Mutation {
    createMoods(input: MoodsInput!, typeId: Int!): Moods
    createMoodTypes(input: MoodTypeInput) : MoodType
}